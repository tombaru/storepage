nav.nav
	.container
		- var items = ["Планшеты", "Мобильные телефоны", "МР3 плееры", "Персональные компьютеры", "Ноутбуки", "Аксессуары", "Бытовая техника"]
		ul.nav__list
			each item in items
				li(class="nav__item nav__item_" + ((item == "Мобильные телефоны") ? 'active' : ''))
					a(href="#").nav__link= item